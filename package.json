{
  "name": "custom-resource-provider",
  "version": "0.0.3",
  "description": "TypeScript/Node.js - Boilerplate for creating, testing and deploying Lambda-backed, AWS Cloudformation Custom Resources. Includes several examples and some useful resources that make (Enterprise, but works for everyone) deployments easier.",
  "scripts": {
    "clean:build": "rm -rf build 2> /dev/null",
    "clean:nm": "rm -rf node_modules 2> /dev/null",
    "clean:prod": "npm run clean-nm && rm -rf dist 2> /dev/null",
    "build": "rm -rf dist && node_modules/.bin/tsc --project tsconfig.build.json",
    "build:prod": "rm -rf dist && NODE_ENV=production node_modules/.bin/tsc --project tsconfig.build.json",
    "debug": "npm run ts --inspect-brk",
    "deploy": "npm run ts bin/deploy",
    "deploy:prod": "npm run clean-prod && npm install && NODE_ENV=production npm run deploy",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "node": "node",
    "test": "NODE_ENV=testing node_modules/.bin/jest",
    "ts": "npm run node -- -r ts-node/register",
    "webpack": "npm run ts node_modules/.bin/webpack"
  },
  "keywords": [],
  "author": "Matthew Keil",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "shortid": "^2.2.15",
    "debug": "^4.1.1",
    "@types/aws-lambda": "^8.10.51"
  },
  "devDependencies": {
    "@types/archiver": "^3.1.0",
    "@types/debug": "^4.1.5",
    "@types/jest": "^25.2.3",
    "@types/shortid": "0.0.29",
    "@types/uglifyjs-webpack-plugin": "^1.1.0",
    "@types/uuid": "^7.0.3",
    "@types/webpack": "^4.41.13",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "@vuepress/plugin-active-header-links": "^1.4.1",
    "@vuepress/plugin-back-to-top": "^1.4.1",
    "@vuepress/plugin-google-analytics": "^1.4.1",
    "@vuepress/plugin-last-updated": "^1.4.1",
    "@vuepress/plugin-medium-zoom": "^1.4.1",
    "@vuepress/plugin-nprogress": "^1.4.1",
    "@vuepress/plugin-register-components": "^1.4.1",
    "archiver": "^4.0.1",
    "aws-sdk": "^2.678.0",
    "axios": "^0.19.2",
    "cloudform": "^5.0.1",
    "dotenv": "^8.2.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^26.0.1",
    "jszip": "^3.4.0",
    "node-zip": "^1.1.1",
    "nomad-devops": "git+https://github.com/matthewkeil/nomad-devops.git",
    "prettier": "^1.17.1",
    "shebang-loader": "0.0.1",
    "string-replace-loader": "^2.3.0",
    "ts-jest": "^26.0.0",
    "ts-loader": "^7.0.4",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "uuid": "^8.0.0",
    "vue-toasted": "^1.1.28",
    "vuepress": "^1.4.1",
    "vuepress-plugin-clean-urls": "^1.1.1",
    "vuepress-plugin-container": "^2.1.3",
    "vuepress-plugin-redirect": "^1.2.3",
    "vuepress-plugin-smooth-scroll": "0.0.9",
    "vuepress-plugin-typescript": "^0.2.0",
    "vuepress-types": "^0.9.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}

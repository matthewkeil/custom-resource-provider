{
  "name": "nomad-custom-resources",
  "version": "0.0.1",
  "description": "CloudFormation Custom Resources built with TypeScript",
  "main": "index.js",
  "scripts": {
    "clean-dist": "rm -rf dist 2> /dev/null",
    "clean-build": "rm -rf build 2> /dev/null",
    "clean-nm": "rm -rf node_modules 2> /dev/null",
    "build-dev": "npm run webpack",
    "build": "NODE_ENV=production npm run webpack",
    "deploy": "npm run ts bin/deploy",
    "deploy-prod": "npm run clean-nm && npm install && npm run build-prod && npm run deploy",
    "test": "NODE_ENV=testing node_modules/.bin/jest",
    "webpack": "npm run ts node_modules/.bin/webpack",
    "node": "node",
    "ts": "npm run node -- -r ts-node/register",
    "debug": "npm run ts --inspect-brk"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "shortid": "^2.2.15",
    "debug": "^4.1.1"
  },
  "devDependencies": {
    "@types/archiver": "^3.1.0",
    "@types/aws-lambda": "^8.10.51",
    "@types/debug": "^4.1.5",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^25.2.3",
    "@types/jszip": "^3.4.1",
    "@types/shortid": "0.0.29",
    "@types/uuid": "^7.0.3",
    "@types/webpack": "^4.41.13",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "axios": "^0.19.2",
    "aws-sdk": "^2.678.0",
    "archiver": "^4.0.1",
    "cloudform": "^5.0.1",
    "dotenv": "^8.2.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^26.0.1",
    "jszip": "^3.4.0",
    "node-zip": "^1.1.1",
    "nomad-devops": "git+https://github.com/matthewkeil/nomad-devops.git",
    "prettier": "^1.17.1",
    "shebang-loader": "0.0.1",
    "string-replace-loader": "^2.3.0",
    "ts-jest": "^26.0.0",
    "ts-loader": "^7.0.4",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.2",
    "uuid": "^8.0.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
